{
"manifest_version": 2,
    "name": "Millennial Reality Generator",
    "description": "Replaces various text strings with phrases that cater to millennials.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}